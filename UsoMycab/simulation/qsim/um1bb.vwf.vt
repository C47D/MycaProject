module um1bb_vlg_vec_tst();
// constants                                           
// general purpose registers
reg CK;
reg LC;
reg LS;
reg M;
reg SG;
reg SL;
// wires                                               
wire CL;
wire CLK;
wire CN;
wire DE;
wire [3:0] SCP;
wire SV;

wire sampler;                             

// assign statements (if any)                          
um1bb i1 (
// port map - connection between master ports and signals/registers   
	.CK(CK),
	.CL(CL),
	.CLK(CLK),
	.CN(CN),
	.DE(DE),
	.LC(LC),
	.LS(LS),
	.M(M),
	.SCP(SCP),
	.SG(SG),
	.SL(SL),
	.SV(SV)
);

// CK
always
begin
	CK = 1'b0;
	CK = #10000 1'b1;
	#10000;
end 

// M
initial
begin
	M = 1'b1;
	M = #250000 1'b0;
end 

// SL
initial
begin
	SL = 1'b0;
end 

// SG
initial
begin
	SG = 1'b0;
end 

// LS
initial
begin
	LS = 1'b0;
end 

// LC
initial
begin
	LC = 1'b0;
end 

um1bb_vlg_sample_tst tb_sample (
	.CK(CK),
	.LC(LC),
	.LS(LS),
	.M(M),
	.SG(SG),
	.SL(SL),
	.sampler_tx(sampler)
);

um1bb_vlg_check_tst tb_out(
