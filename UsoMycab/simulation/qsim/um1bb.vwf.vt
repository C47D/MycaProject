	$display("| expected CL = %b | expected CLK = %b | expected CN = %b | expected DE = %b | expected SCP = %b | expected SV = %b | ",CL_expected_prev,CLK_expected_prev,CN_expected_prev,DE_expected_prev,SCP_expected_prev,SV_expected_prev);
